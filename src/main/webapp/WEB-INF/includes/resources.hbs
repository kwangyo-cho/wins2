<script type="text/javascript">
requirejs( [
		"/lib/loglevel.min.js",
		"/lib/lodash.js",
		"/lib/axios.js",
		"/lib/adc.js",
		"/lib/simple-scrollbar.min.js"
	],
	function (log, _, axios, adc) {
		window.log = log.noConflict();
		log.setLevel("trace");
		log.info("global require complete");
		_.forOwn(requirejs.s.contexts._.urlFetched, function(value, key) {
			log.debug(key + (value ? " loaded" : " error"));
		});
		requirejs(["/js/common/menu.js"], function () {
			window.menu.init(document.querySelector("#_menu"));
		});
		adc.init();	// dom loaded Event bind
	}
);
</script>
